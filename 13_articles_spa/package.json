{
  "name": "articles-spa",
  "version": "1.0.0",
  "description": "Elm SPA with routing and an Express API server",
  "scripts": {
    "start": "concurrently \"elm-live src/Main.elm --open --pushstate -- --output=elm.js --debug\" \"node server.js\"",
    "build": "elm make src/Main.elm --output=elm.js",
    "server": "node server.js"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "elm-live": "^4.0.2"
  }
}
